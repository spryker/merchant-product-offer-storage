<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="ProductConcreteProductOffersStorage">
        <property name="idProductConcreteProductOffersStorage" type="int"/>
        <property name="concreteSku" type="string"/>
        <property name="data" type="string[]"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="ProductOfferStorage">
        <property name="idProductOfferStorage" type="int"/>
        <property name="productOfferReference" type="string"/>
        <property name="data" type="array"/>
        <property name="key" type="string"/>
    </transfer>

    <transfer name="ProductOfferViewCollection">
        <property name="ProductOfferViews" type="ProductOfferView[]" singular="ProductOfferView"/>
    </transfer>

    <transfer name="ProductOfferView">
        <property name="idProductOffer" type="int"/>
        <property name="productOfferReference" type="string"/>
        <property name="idMerchant" type="int"/>
    </transfer>

    <transfer name="ProductConcreteProductOffersStorageCriteriaFilter">
        <property name="productConcreteProductOffersStorageIds" type="int[]"/>
        <property name="filter" type="Filter"/>
    </transfer>

    <transfer name="ProductOfferStorageCriteriaFilter">
        <property name="productOfferStorageIds" type="int[]"/>
        <property name="filter" type="Filter"/>
    </transfer>

    <transfer name="Filter">
        <property name="offset" type="int"/>
        <property name="limit" type="int"/>
    </transfer>

</transfers>
